{
    "$schema": "https://github.com/fastfetch-cli/fastfetch/raw/dev/doc/json_schema.json",
    "logo": {
        "color": { "1": "38;2;255;147;0"},
        "source": "~/.config/fastfetch/logo.txt",
        "padding": {
            "right": 4
        }
    },
    "display": {
        "separator": " → ",
        "color": {
            "separator": "cyan",
        }
    },
    "modules": [
        {
            "type": "title",
            "format":   "╭─ {#}{user-name} ──────────────────────────────────────────╮"
        },
        {
            "type": "custom",
            "format": "│                                                 │"
        },
        {
            "type": "custom",
            "format": "│                 🎷🐛🎷🐛🎷🐛                    │"
        },
        {
            "type": "custom",
            "format": "│                                                 │"
        },
        {
            "type": "custom",
            "format": "╰─────────────────────────────────────────────────╯"
        },
        {
            "type": "custom",
            "format": "╭─ Software ──────────────────────────────────────╮"
        },
        {
            "type": "os",
            "key": "│ OS",
            "format":      "{#}{pretty-name}                         │",
            "keyColor": "cyan"
        },
        {
            "type": "host",
            "key": "│ Host",
            "format":      "{#}{name}     │",
            "keyColor": "cyan"
        },
        {
            "type": "kernel",
            "key": "│ Kernel",
            "format":      "{#}{sysname} {#}{release} │",
            "keyColor": "cyan"
        },
        {
            "type": "shell",
            "key": "│ Shell",
            "format":      "{#}{pretty-name}                                    │",
            "keyColor": "cyan"
        },
        {
            "type": "packages",
            "key": "│ DPKG",
            "format":      "{#}{dpkg} packages                             │",
            "keyColor": "cyan"
        },
        {
            "type": "packages",
            "key": "│ Snap",
            "format":      "{#}{snap} packages                               │",
            "keyColor": "cyan"
        },
        {
            "type": "custom",
            "format": "╰─────────────────────────────────────────────────╯"
        },
        {
            "type": "custom",
            "format": "╭─ Hardware ──────────────────────────────────────╮"
        },
        {
            "type": "display",
            "key": "│ Display",
            "format":      "{#}{width}x{#}{height} {#}{refresh-rate}Hz                        │",
            "keyColor": "yellow"
        },
        {
            "type": "cpu",
            "key": "│ CPU",
            "format":      "{#}{name} ({#}{cores-physical}) {#}{freq-base}   │",
            "keyColor": "yellow"
        },
        {
            "type": "gpu",
            "key": "│ GPU",
            "format":      "{#}{name} {#}{dedicated-total}      │",
            "keyColor": "yellow"
        },
        {
            "type": "memory",
            "key": "│ Memory",
            "format":      "{#}{used} / {#}{total} ({#}{percentage})             │",
            "keyColor": "yellow"
        },
        {
            "type": "swap",
            "key": "│ Swap",
            "format":      "{#}{used} / {#}{total} ({#}{percentage})                      │",
            "keyColor": "yellow"
        },
        {
            "type": "custom",
            "format": "╰─────────────────────────────────────────────────╯"
        },
        {
            "type": "custom",
            "format": "╭─ Colours ───────────────────────────────────────╮"
        },
        {
            "type": "colors",
            "key": "│ Yo",
            "symbol": "circle",
            "keyColor": "38;2;255;0;119"
        },
        {
            "type": "custom",
            "format": "╰─────────────────────────────────────────────────╯"
        },
  ]
}
